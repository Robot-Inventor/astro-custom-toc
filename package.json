{
  "name": "astro-custom-toc",
  "version": "3.0.2",
  "description": "Astro Integration to generate a customizable table of contents",
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier ./src/ --write",
    "format:check": "prettier ./src/ --check",
    "lint": "knip && npx eslint ./src/**/*.ts",
    "version": "npm run build && git add .",
    "changeset-version": "changeset version && npm run version",
    "changeset-publish": "npm run build && changeset publish"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robot-Inventor/astro-custom-toc.git"
  },
  "keywords": [
    "astro",
    "withastro",
    "ui",
    "remark",
    "markdown",
    "astro-integration"
  ],
  "author": "Robot-Inventor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robot-Inventor/astro-custom-toc/issues"
  },
  "homepage": "https://github.com/Robot-Inventor/astro-custom-toc#readme",
  "dependencies": {
    "@astrojs/markdown-remark": "^6.0.0",
    "@types/hast": "^3.0.4",
    "rehype-custom-toc": "^1.0.0",
    "remark-comment": "^1.0.0",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@robot-inventor/eslint-config": "^11.0.0",
    "@robot-inventor/tsconfig-base": "^7.0.0",
    "astro": "^5.1.1",
    "eslint": "^9.0.0",
    "knip": "^5.80.2",
    "prettier": "^3.2.5",
    "typescript": "^5.9.2",
    "vfile": "^6.0.3"
  }
}
